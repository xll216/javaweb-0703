<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!--学生就业信息表-->
    <class name="com.lanou.crm.graduate.domain.Graduate">
        <id name="granduateId" length="32">
            <generator class="native"/>
        </id>
        <property name="studentId" length="32"/>
        <property name="companyName" length="100"/>
        <property name="salary" length="50"/>
        <property name="post" length="50"/>
        <property name="entryTime"/>
        <property name="remark" length="500"/>
        <property name="classID" length="50"/>

    </class>

</hibernate-mapping>